{
    "module_name": "Core",
    "template_file": "./config_example/piped_cpu/vga_debugger_template.txt",
    "output_dir": "./output/",
    "mem_file": "vga_debugger.mem",
    "dbg_header": "vga_debugger.vh",
    "header_lines": 1,
    "block_prefix": {
        "Id": "IfId_",
        "Ex": "IdEx_",
        "Ma": "ExMa_",
        "Wb": "MaWb_"
    },
    "block_suffix": {
        "CSR": "_o"
    },
    "len_bits": {
        "Ex": {
            "rd": 5,
            "rs1": 5,
            "rs2": 5,
            "alu_ctrl": 4,
            "cmp_ctrl": 3,
            "evec": 2
        },
        "Ma": {
            "rd": 5,
            "evec": 3
        },
        "Wb": {
            "rd": 5,
            "evec": 4
        }
    },
    "wire_prefix": {
        "*regs": "",
        "Ex": {
            "forward_rs1": "Ex_",
            "forward_rs2": "Ex_"
        }
    },
    "submodule": [
        {
            "name": "RegFile",
            "wires": {
                "*regs": []
            }
        },
        {
            "name": "Csr",
            "wires": {
                "CSR": [
                    "mcause", "mepc", "mstatus", "mtval", "mtvec", "mie", "mip"
                ]
            }
        }
    ],
    "wire_group": [
        {
        	"name": "regs",
            "wires": {
                "Id": [
                    "x0", "ra", "sp", "gp", "tp", "t0", "t1", "t2",
                    "s0", "s1", "a0", "a1", "a2", "a3", "a4", "a5",
                    "a6", "a7", "s2", "s3", "s4", "s5", "s6", "s7",
                    "s8", "s9", "s10", "s11", "t3", "t4", "t5", "t6"
                ]
            }
        }
    ],
    "wire_name": {
        "If": {
            "inst": "imem_o_data"
        },
        "Id": {
            "x0": "regs[0]",
            "ra": "regs[1]",
            "sp": "regs[2]",
            "gp": "regs[3]",
            "tp": "regs[4]",
            "t0": "regs[5]",
            "t1": "regs[6]",
            "t2": "regs[7]",
            "s0": "regs[8]",
            "s1": "regs[9]",
            "a0": "regs[10]",
            "a1": "regs[11]",
            "a2": "regs[12]",
            "a3": "regs[13]",
            "a4": "regs[14]",
            "a5": "regs[15]",
            "a6": "regs[16]",
            "a7": "regs[17]",
            "s2": "regs[18]",
            "s3": "regs[19]",
            "s4": "regs[20]",
            "s5": "regs[21]",
            "s6": "regs[22]",
